itch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":250,"indent":0,"parameters":[{"name":"Move1","volume":90,"pitch":100,"pan":0}]},{"code":201,"indent":0,"parameters":[0,2,10,1,0,0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":1,"walkAnime":true}],"x":13,"y":24},{"id":4,"name":"ミノタウロス","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"Monster2","direction":2,"pattern":1,"characterIndex":3},"list":[{"code":230,"indent":0,"parameters":[10]},{"code":241,"indent":0,"parameters":[{"name":"11_Forest of the witch","volume":80,"pitch":100,"pan":0}]},{"code":223,"indent":0,"parameters":[[0,0,0,0],60,true]},{"code":230,"indent":0,"parameters":[15]},{"code":213,"indent":0,"parameters":[5,6,true]},{"code":117,"indent":0,"parameters":[126]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]伊蕾诺"]},{"code":401,"indent":0,"parameters":["「呜呜……」"]},{"code":117,"indent":0,"parameters":[131]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「哼，终究是个魅魔。你这个种族的脑子里只装着魅惑男性的东西。"]},{"code":401,"indent":0,"parameters":["　只要我使出暴力，你就没有道理可以赢过我。伊蕾诺。」"]},{"code":117,"indent":0,"parameters":[126]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]伊蕾诺"]},{"code":401,"indent":0,"parameters":["「米诺斯……你明知道自己做出这种事情会有什么下场吗？"]},{"code":401,"indent":0,"parameters":["　魔王大人不可能原谅你对魔王大人的叛逆。」"]},{"code":117,"indent":0,"parameters":[131]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「抱歉，我已经不打算继续侍奉现在的魔王大人了。」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「一问之下，魔王大人不是正以侵略为目的，"]},{"code":401,"indent":0,"parameters":["　在各地建设城市吗？」"]},{"code":117,"indent":0,"parameters":[126]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]伊蕾诺"]},{"code":401,"indent":0,"parameters":["「那又怎样？只要在各地建立据点，支配那些地方，"]},{"code":401,"indent":0,"parameters":["　最后不就可以征服世界了吗？"]},{"code":401,"indent":0,"parameters":["　魔王大人的做法哪里有问题了？」"]},{"code":117,"indent":0,"parameters":[131]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「如果支配的是那个地方的人们，那就无所谓。"]},{"code":401,"indent":0,"parameters":["　但是，听说建造城镇的目的"]},{"code":401,"indent":0,"parameters":["　不是与在那个地方的人们共存吗？」"]},{"code":117,"indent":0,"parameters":[127]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]伊蕾诺"]},{"code":401,"indent":0,"parameters":["「……比起用恐怖支配，共存的方式不是能维持更久吗。"]},{"code":401,"indent":0,"parameters":["　我并不讨厌魔王大人现在的政策。」"]},{"code":250,"indent":0,"parameters":[{"name":"Blow4","volume":90,"pitch":100,"pan":0}]},{"code":117,"indent":0,"parameters":[131]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「你忘记人类对吾等做过什么了吗！"]},{"code":401,"indent":0,"parameters":["　人类什么的，只要把他们当作奴隶饲养就行了！"]},{"code":401,"indent":0,"parameters":["　事实上克拉西斯帝国之类的，就对吾等做过这种事情吧！」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「尽管如此，您却不去建立一个为魔族而存在的国家，"]},{"code":401,"indent":0,"parameters":["　反而以与人类共存为目标……我对于魔王大人感到失望！」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「从今以后我将成为新的魔王，"]},{"code":401,"indent":0,"parameters":["　支配人类，把他们作为奴隶，建立我们魔族位于顶点的世界。」"]},{"code":117,"indent":0,"parameters":[127]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]伊蕾诺"]},{"code":401,"indent":0,"parameters":["「……我说啊，那样历史就又会重演了，你为什么不明白呢。"]},{"code":401,"indent":0,"parameters":["　我就是讨厌你们魔族这种头脑简单四肢发达的思考方式，"]},{"code":401,"indent":0,"parameters":["　才会和你们划清界限呢——」"]},{"code":117,"indent":0,"parameters":[129]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]伊蕾诺"]},{"code":401,"indent":0,"parameters":["「不过，魔王大人稍微变了一些呢。"]},{"code":401,"indent":0,"parameters":["　接受了失败，开始探索新的政策和可能性。"]},{"code":401,"indent":0,"parameters":["　老实说，比起前任魔王大人，我更喜欢现在的魔王大人。」"]},{"code":117,"indent":0,"parameters":[131]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「……哼，我真是越来越和你不合呢。伊蕾诺。」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「不管怎样，一旦掌握了这座魔王城，"]},{"code":401,"indent":0,"parameters":["　魔王大人就会回来这里吧。"]},{"code":117,"indent":0,"parameters":[127]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]伊蕾诺"]},{"code":401,"indent":0,"parameters":["那时，我就会打倒魔王大人，成为新的魔王。」"]},{"code":117,"indent":0,"parameters":[131]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「我先说好了，你赢不了魔王大人的！」"]},{"code":401,"indent":0,"parameters":["　「那可不好说？以前的魔王大人暂且不论，"]},{"code":101,"indent":0,"parameters":["",0,0,2,"米诺斯"]},{"code":401,"indent":0,"parameters":["「所以我才要你\\c[27]为此采取对策\\c[0]。」"]},{"code":117,"indent":0,"parameters":[126]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]伊蕾诺"]},{"code":401,"indent":0,"parameters":["「……诶？」"]},{"code":117,"indent":0,"parameters":[127]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]伊蕾诺"]},{"code":401,"indent":0,"parameters":["「所以，我要用你来测试一下」"]},{"code":401,"indent":0,"parameters":["　（啊，这，说不定有点不妙……"]},{"code":117,"indent":0,"parameters":[1]},{"code":242,"indent":0,"parameters":[3]},{"code":230,"indent":0,"parameters":[10]},{"code":223,"indent":0,"parameters":[[-255,-255,-255,0],60,true]},{"code":230,"indent":0,"parameters":[100]},{"code":211,"indent":0,"parameters":[1]},{"code":201,"indent":0,"parameters":[0,6,20,10,2,0]},{"code":223,"indent":0,"parameters":[[0,0,0,0],50,tru