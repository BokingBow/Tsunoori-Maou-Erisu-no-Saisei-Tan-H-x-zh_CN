:false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"erisu05A","direction":4,"pattern":1,"characterIndex":0},"list":[{"code":230,"indent":0,"parameters":[1]},{"code":205,"indent":0,"parameters":[4,{"list":[{"code":42,"parameters":[160],"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":42,"parameters":[160],"indent":null}]},{"code":230,"indent":0,"parameters":[10]},{"code":223,"indent":0,"parameters":[[34,-34,-68,170],50,true]},{"code":230,"indent":0,"parameters":[20]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[2]艾莉丝"]},{"code":401,"indent":0,"parameters":["「诶——那，里克觉得人类与魔族能够共同生存的世界"]},{"code":401,"indent":0,"parameters":["　才是最好的」"]},{"code":117,"indent":0,"parameters":[398]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[3]里克"]},{"code":401,"indent":0,"parameters":["「是啊。人类有属于人类的领土，魔族有属于魔族的领土。"]},{"code":401,"indent":0,"parameters":["　能够彼此不发生纠纷地生活下去才是最好的吧。"]},{"code":401,"indent":0,"parameters":["　应该没有什么理由能够随意地互相战斗吧」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[2]艾莉丝"]},{"code":401,"indent":0,"parameters":["「……但是，确实这么一说的话或许就是这样」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[3]里克"]},{"code":401,"indent":0,"parameters":["「嗯。所以，如果我拿回勇者的称号，"]},{"code":401,"indent":0,"parameters":["　而艾莉丝继承魔王的称号的话，"]},{"code":401,"indent":0,"parameters":["　\\c[14]就由我们来创造彼此的种族能够共同生存的世界\\c[0]吧」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[2]艾莉丝"]},{"code":401,"indent":0,"parameters":["「啊哈哈！或许不错！」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[3]里克"]},{"code":401,"indent":0,"parameters":["「哈哈，有赞同我想法的魔族孩子在真是帮大忙了」"]},{"code":117,"indent":0,"parameters":[1]},{"code":213,"indent":0,"parameters":[0,8,true]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[2]艾莉丝"]},{"code":401,"indent":0,"parameters":["「……话说，里克现在好像是没有肉体的\\c[3]幽灵\\c[0]，"]},{"code":401,"indent":0,"parameters":["　然后不久后就要附身到那个叫米莉亚的家伙身上，"]},{"code":401,"indent":0,"parameters":["　妨碍她的勇者行动是吧？」"]},{"code":117,"indent":0,"parameters":[398]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[3]里克"]},{"code":401,"indent":0,"parameters":["「是啊。我打算巧妙地削弱那家伙的勇者之力，"]},{"code":401,"indent":0,"parameters":["　最后让你的父亲打倒她……只要一切顺利的话。」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[2]艾莉丝"]},{"code":401,"indent":0,"parameters":["「……这样啊。」"]},{"code":117,"indent":0,"parameters":[1]},{"code":213,"indent":0,"parameters":[0,8,true]},{"code":213,"indent":0,"parameters":[0,9,true]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[2]艾莉丝"]},{"code":401,"indent":0,"parameters":["「欸欸！以后我可以叫里克\\c[3]父亲大人\\c[0]吗？」"]},{"code":117,"indent":0,"parameters":[398]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[3]里克"]},{"code":401,"indent":0,"parameters":["「咦？这倒是无所谓……"]},{"code":401,"indent":0,"parameters":["　不过，艾莉丝的父亲不是那个魔王吗？」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[2]艾莉丝"]},{"code":401,"indent":0,"parameters":["「没关系没关系。因为父亲大人说\\c[27]我是魅魔的混血儿\\c[0]，"]},{"code":401,"indent":0,"parameters":["　所以不怎么期待我成为继承人。」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[2]艾莉丝"]},{"code":401,"indent":0,"parameters":["「更重要的是，里克是第一个会这样好好告诉吾辈的人，"]},{"code":401,"indent":0,"parameters":["　对我来说，里克就像是父亲大人一样！」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[3]里克"]},{"code":401,"indent":0,"parameters":["「是吗。那，艾莉丝想怎么叫都行。」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[2]艾莉丝"]},{"code":401,"indent":0,"parameters":["「嗯♪」"]},{"code":117,"indent":0,"parameters":[1]},{"code":213,"indent":0,"parameters":[0,3,true]},{"code":230,"indent":0,"parameters":[5]},{"code":223,"indent":0,"parameters":[[-255,-255,-255,0],60,true]},{"code":230,"indent":0,"parameters":[10]},{"code":101,"indent":0,"parameters":["",0,1,1,""]},{"code":401,"indent":0,"parameters":["就这样，吾辈与里克共度了很长一段时间。"]},{"code":401,"indent":0,"parameters":["实际上，与里克共度的时间比与亲生父亲共度的时间还要长。"]},{"code":401,"indent":0,"parameters":["从这个意义上来说，我的父亲是里克。"]},{"code":101,"indent":0,"parameters":["",0,1,1,""]},{"code":401,"indent":0,"parameters":["可是，这一天终于来了。"]},{"code":401,"indent":0,"parameters":["里克终于可以附身在杀害自己的那小丫头的身体上，打倒那个女人了。"]},{"code":101,"indent":0,"parameters":["",0,1,1,""]},{"code":401,"indent":0,"parameters":["然而，命运却站在父亲那边。"]},{"code":230,"indent":0,"parameters":[30]},{"code":250,"indent":0,"parameters":[{"name":"Collapse1","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[5]魔王"]},{"code":401,"indent":0,"parameters":["「呜啊啊啊啊啊啊啊啊啊啊！！！」"]},{"code":230,"indent":0,"parameters":[20]},{"code":117,"indent":0,"parameters":[387]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[27]勇者公主"]},{"code":401,"indent":0,"parameters":["「啊哈哈哈哈！你终究不是得到勇者之力的我的对手呢！」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[3]里克"]},{"code":401,"indent":0,"parameters":["（呜，魔王……我们的计划果然不顺利吗？）"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[27]勇者公主"]},{"code":401,"indent":0,"parameters":["「那么，接下来就是……收拾附身在我身上的蝼蚁了。」"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[3]里克"]},{"code":401,"indent":0,"parameters":["『呜呃！？米莉亚，你注意到了……！"]},{"code":401,"indent":0,"parameters":["　等、等一下！等一下啊！至少听我说――』"]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[27]勇者公主"]},{"code":401,"indent":0,"parameters":["「才不等☆再见啦，大哥哥❤」"]},{"code":117,"indent":0,"parameters":[1]},{"code":230,"indent":0,"parameters":[10]},{"code":250,"indent":0,"parameters":[{"name":"Collapse2","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,0,2,"\\c[3]里克"]},{"code":401,"indent":0,"parameters":["\\{「呜哇啊啊啊啊啊啊啊啊啊！！！」"]},{"code":230,"indent":0,"parameters":[100]},{"code":121,"indent":0,"parameters":[213,213,0]},{"code":230,"indent":0,"parameters":[1]},{"code":211,"indent":0,"parameters":[1]},{"code":230,"indent":0,"parameters":[1]},{"code":201,"indent":0,"parameters":[0,183,12,15,8,0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":