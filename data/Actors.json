[null,{"id":1,"battlerName":"","characterIndex":0,"characterName":"erisu01","classId":1,"equips":[2,0,45,3,0,2],"faceIndex":0,"faceName":"ActorEX1","traits":[{"code":52,"dataId":13,"value":1},{"code":52,"dataId":14,"value":1},{"code":51,"dataId":13,"value":1}],"initialLevel":1,"maxLevel":99,"name":"艾莉丝","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>\n","profile":"魔王大人败给勇者，回过神来就回到了征服世界的起始之日。\n反省了以前失败的经验，重新执行征服世界。由于被折断了角而变弱。"},{"id":2,"battlerName":"","characterIndex":0,"characterName":"ritti","classId":3,"equips":[30,0,46,45,0,15],"faceIndex":0,"faceName":"ActorEX2","traits":[{"code":52,"dataId":13,"value":1},{"code":52,"dataId":14,"value":1}],"initialLevel":3,"maxLevel":99,"name":"里奇","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"魔王四天王之一，性格十分消极，总是畏畏缩缩。\n对魔王的忠诚心很高。在性知识方面，有着宅系的奇怪知识。"},{"id":3,"battlerName":"","characterIndex":0,"characterName":"ire-nu01","classId":4,"equips":[34,0,50,139,0,22],"faceIndex":4,"faceName":"ActorEX2","traits":[{"code":52,"dataId":13,"value":1},{"code":52,"dataId":14,"value":1}],"initialLevel":9,"maxLevel":99,"name":"伊蕾诺","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"魔王四天王之一。自称萨库拉斯族的女王，但是真伪不明。\n忠诚于魔王大人的命令，但基本上很懒惰。喜欢优先考虑对自己有利的事情。"},{"id":4,"battlerName":"","characterIndex":0,"characterName":"yukionnaA","classId":27,"equips":[47,0,68,102,228,22],"faceIndex":0,"faceName":"ActorEX7","traits":[{"code":52,"dataId":13,"value":1},{"code":14,"dataId":11,"value":1},{"code":14,"dataId":65,"value":1}],"initialLevel":32,"maxLevel":99,"name":"雪女","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"传说的雪女。拥有对水・冰的完全抗性，会使用许多攻击魔法。另外回复・削弱技能都会用。\n唯一不能使用火魔法，火是弱点。另外意外地攻击力也很高，通过增加战士系的特性，物理攻击也行。"},{"id":5,"battlerName":"","characterIndex":0,"characterName":"dorago01","classId":5,"equips":[70,0,57,64,234,31],"faceIndex":0,"faceName":"ActorEX5","traits":[{"code":52,"dataId":13,"value":1},{"code":52,"dataId":14,"value":1}],"initialLevel":20,"maxLevel":99,"name":"德拉戈","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"魔王军四天王之一。肉搏战实力在魔王军中也属于上位。\n认为力量就是力量，不喜欢麻烦的作战，所以对魔王目前的方针没什么兴趣。"},{"id":6,"battlerName":"","characterIndex":0,"characterName":"ruvi01A","classId":20,"equips":[0,0,0,3,0,38],"faceIndex":4,"faceName":"ActorEX1","traits":[{"code":52,"dataId":13,"value":1},{"code":51,"dataId":14,"value":1},{"code":52,"dataId":17,"value":1}],"initialLevel":1,"maxLevel":1,"name":"露维","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>\n","profile":"出现在艾莉丝面前的神秘少女。是个有些不谙世事的天然女孩。\n为了召唤灭之波，她协助艾莉丝，但她的目的究竟是……？"},{"id":7,"battlerName":"","characterIndex":0,"characterName":"rikku01","classId":2,"equips":[23,0,64,115,0],"faceIndex":0,"faceName":"ActorEX3B","traits":[{"code":51,"dataId":14,"value":1},{"code":52,"dataId":17,"value":1}],"initialLevel":30,"maxLevel":99,"name":"里克","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"曾经有望获得勇者称号的青年。\n他原本注定会被米莉亚陷害而死，然而……？"},{"id":8,"battlerName":"","characterIndex":0,"characterName":"","classId":1,"equips":[0,0,0,0,0],"faceIndex":0,"faceName":"","traits":[],"initialLevel":1,"maxLevel":99,"name":"","nickname":"","note":"","profile":""},{"id":9,"battlerName":"","characterIndex":0,"characterName":"","classId":1,"equips":[0,0,0,0,0],"faceIndex":0,"faceName":"","traits":[],"initialLevel":1,"maxLevel":99,"name":"","nickname":"","note":"","profile":""},{"id":10,"battlerName":"","characterIndex":0,"characterName":"","classId":1,"equips":[0,0,0,0,0],"faceIndex":0,"faceName":"","traits":[],"initialLevel":1,"maxLevel":99,"name":"处女","nickname":"","note":"","profile":"艾莉丝的处女"},{"id":11,"battlerName":"","characterIndex":0,"characterName":"erisu01","classId":1,"equips":[82,70,64,89,219,2],"faceIndex":0,"faceName":"ActorEX1","traits":[{"code":53,"dataId":1,"value":1},{"code":53,"dataId":2,"value":1},{"code":53,"dataId":3,"value":1},{"code":53,"dataId":4,"value":1},{"code":53,"dataId":5,"value":1},{"code":53,"dataId":6,"value":1},{"code":62,"dataId":0,"value":1}],"initialLevel":99,"maxLevel":99,"name":"艾莉丝","nickname":"","note":"","profile":"傲慢又任性，吾辈系魔王。对于自己的力量过于自信，想用\n粗暴的作战来征服世界……。"},{"id":12,"battlerName":"","characterIndex":0,"characterName":"","classId":1,"equips":[0,0,0,0,0],"faceIndex":0,"faceName":"","traits":[],"initialLevel":1,"maxLevel":99,"name":"处女","nickname":"里奇","note":"","profile":"里奇的处女"},{"id":13,"battlerName":"","characterIndex":0,"characterName":"","classId":1,"equips":[0,0,0,0,0],"faceIndex":0,"faceName":"","traits":[],"initialLevel":1,"maxLevel":99,"name":"处女","nickname":"露维","note":"","profile":"露维的处女"},{"id":14,"battlerName":"","characterIndex":0,"characterName":"","classId":1,"equips":[0,0,0,0,0],"faceIndex":0,"faceName":"","traits":[],"initialLevel":1,"maxLevel":99,"name":"处女","nickname":"伊蕾诺","note":"","profile":"伊蕾诺的处女"},{"id":15,"battlerName":"","characterIndex":1,"characterName":"ETNR_EX076","classId":8,"equips":[0,0,0,3,0],"faceIndex":0,"faceName":"ActorEX6","traits":[{"code":52,"dataId":15,"value":1},{"code":52,"dataId":16,"value":1},{"code":52,"dataId":14,"value":1}],"initialLevel":1,"maxLevel":50,"name":"哥布林","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"只要是不洁且性欲极高的女人，就会立刻对其播种，让其成为孕袋的魔物。\n战斗能力不高。有总比没有好程度。"},{"id":16,"battlerName":"","characterIndex":2,"characterName":"Monster2","classId":9,"equips":[84,0,0,3,0],"faceIndex":1,"faceName":"ActorEX6","traits":[{"code":52,"dataId":16,"value":1},{"code":52,"dataId":14,"value":1}],"initialLevel":3,"maxLevel":35,"name":"兽人","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"拥有强健的肉体，以及与肉体相应的战斗天赋。\n相对而言想法比较有良知，把自己阵营的防御放在第一位考虑。"},{"id":17,"battlerName":"","characterIndex":0,"characterName":"nazo_slime","classId":12,"equips":[0,0,0,3,0],"faceIndex":3,"faceName":"ActorEX6","traits":[{"code":52,"dataId":12,"value":1}],"initialLevel":1,"maxLevel":99,"name":"史莱姆","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"虽然被取笑是全魔物中最弱小的存在，但其实可以成长到最大等级99。\n虽然被称为大器晚成型…？还有史莱姆族拥有完全抗致命一击的特性。"},{"id":18,"battlerName":"","characterIndex":4,"characterName":"Actor2","classId":14,"equips":[0,0,0,3,0],"faceIndex":4,"faceName":"ActorEX6","traits":[],"initialLevel":1,"maxLevel":75,"name":"帮派","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"能力值平均，但拥有非常高的敏捷性和运气。\n具备擅长攻击弱点的技能，如果能灵活运用的话，说不定能成为强大的战力？"},{"id":19,"battlerName":"","characterIndex":0,"characterName":"nikku01","classId":15,"equips":[65,0,51,77,0],"faceIndex":5,"faceName":"ActorEX6","traits":[],"initialLevel":1,"maxLevel":50,"name":"帮派头领","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"统率帮派成员的头领。虽然拥有比帮派成员更高的战斗能力，\n但却有MP完全不会增加的弱点。"},{"id":20,"battlerName":"","characterIndex":6,"characterName":"wingedserpants","classId":13,"equips":[66,0,0,3,228],"faceIndex":6,"faceName":"ActorEX6","traits":[{"code":52,"dataId":14,"value":1}],"initialLevel":2,"maxLevel":40,"name":"河童","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"拥有对水・冰系攻击的抗性，使这些攻击伤害减半的魔物。\n另外，除了学会水・冰系魔法外，也拥有格斗天赋。最大等级40级。"},{"id":21,"battlerName":"","characterIndex":1,"characterName":"$ETNR_001-2B","classId":10,"equips":[52,0,0,3,221],"faceIndex":7,"faceName":"ActorEX6","traits":[{"code":52,"dataId":15,"value":1},{"code":52,"dataId":16,"value":1},{"code":52,"dataId":14,"value":1}],"initialLevel":5,"maxLevel":55,"name":"精英哥布林","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"哥布林的高阶版本。拥有操控火焰魔法的力量。\n而且还拥有对火焰属性的抗性，拥有不会被火焰烧伤的特性。"},{"id":22,"battlerName":"","characterIndex":0,"characterName":"ETNR_EX076","classId":11,"equips":[85,0,0,3,0],"faceIndex":2,"faceName":"ActorEX6","traits":[{"code":52,"dataId":16,"value":1},{"code":52,"dataId":14,"value":1}],"initialLevel":1,"maxLevel":26,"name":"兽人王","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"被评价为兽人之王的魔物。\n最大等级26级，但暗藏着极高的战斗能力。"},{"id":23,"battlerName":"","characterIndex":2,"characterName":"SF_Monster","classId":16,"equips":[0,0,0,3,0],"faceIndex":5,"faceName":"Monster","traits":[{"code":43,"dataId":352,"value":1},{"code":14,"dataId":1,"value":1},{"code":43,"dataId":6,"value":1},{"code":52,"dataId":13,"value":1},{"code":52,"dataId":14,"value":1}],"initialLevel":1,"maxLevel":60,"name":"死灵","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"即使处于无法战斗（HP0）依然可以战斗的特殊角色。只要在战斗中，我方全员的回复能力减半。\n在依然拥有HP的状态下无法使用技能，唯有在无法战斗时才能发挥其真正的价值。"},{"id":24,"battlerName":"","characterIndex":5,"characterName":"SF_Monster","classId":17,"equips":[66,0,0,3,0],"faceIndex":5,"faceName":"SF_Monster","traits":[{"code":14,"dataId":4,"value":1},{"code":14,"dataId":10,"value":1},{"code":14,"dataId":33,"value":1},{"code":14,"dataId":40,"value":1},{"code":14,"dataId":34,"value":1}],"initialLevel":1,"maxLevel":20,"name":"魔像战士","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"拥有极高的防御性能与抗性，从低等级时便能大显身手。\n经常减轻物理伤害20％。但是，最大等级只有20级，很低。"},{"id":25,"battlerName":"","characterIndex":6,"characterName":"ETNR_Sea-anemone","classId":18,"equips":[0,0,0,3,0],"faceIndex":2,"faceName":"Monster2","traits":[{"code":52,"dataId":14,"value":1}],"initialLevel":1,"maxLevel":16,"name":"不可思议的壶","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"拥有极高的防御・魔法防御力，并拥有几乎所有的攻击抗性。\n但另一方面HP很低，弱点是爆击攻击和无视防御的攻击。"},{"id":26,"battlerName":"","characterIndex":3,"characterName":"Evil","classId":19,"equips":[0,0,0,3,0],"faceIndex":3,"faceName":"Evil","traits":[{"code":52,"dataId":14,"value":1}],"initialLevel":1,"maxLevel":50,"name":"禁术师","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"使用禁术的特殊魔族。擅长施放debuff系魔法，\n但也能施展以暗属性为中心的攻击魔法。"},{"id":27,"battlerName":"","characterIndex":1,"characterName":"nazo_slime","classId":21,"equips":[0,0,0,3,0],"faceIndex":4,"faceName":"Monster2","traits":[{"code":52,"dataId":12,"value":1}],"initialLevel":1,"maxLevel":40,"name":"史莱姆治疗","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"擅长使用回复魔法的史莱姆。据说只要有一只就能确保战线安全。\n等级上限到40有些许难点。会完全承受致命一击。"},{"id":28,"battlerName":"","characterIndex":1,"characterName":"young_dragonwalk","classId":22,"equips":[0,0,0,3,0],"faceIndex":0,"faceName":"Monster2","traits":[{"code":52,"dataId":14,"value":1}],"initialLevel":10,"maxLevel":60,"name":"魔兽卡托普勒帕斯","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"拥有debuff和高度格斗能力的魔兽。\n其战斗力甚至被评价为仅次于魔王军的四天王。"},{"id":29,"battlerName":"","characterIndex":7,"characterName":"young_dragonwalk","classId":23,"equips":[0,0,0,3,0],"faceIndex":1,"faceName":"Monster2","traits":[{"code":52,"dataId":14,"value":1}],"initialLevel":20,"maxLevel":60,"name":"圣兽巴隆","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"除了有强大的恢复能力之外，本人本身也拥有高超的战斗力。\n过去曾有过艾莉丝想要挖角却遭到拒绝的经历。"},{"id":30,"battlerName":"","characterIndex":2,"characterName":"Vehicle","classId":24,"equips":[0,0,0,3,0],"faceIndex":3,"faceName":"Monster2","traits":[{"code":52,"dataId":14,"value":1}],"initialLevel":1,"maxLevel":75,"name":"沙罗曼蛇","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"被称为火龙幼体的魔物。一开始战斗力很低，但超过等级20后\n听说会戏剧性地变强……？虽然有火焰和冰的抗性，但弱点是水。"},{"id":31,"battlerName":"","characterIndex":5,"characterName":"Monster","classId":25,"equips":[0,0,0,3,0],"faceIndex":6,"faceName":"Monster2","traits":[{"code":43,"dataId":352,"value":1},{"code":14,"dataId":1,"value":1},{"code":43,"dataId":6,"value":1},{"code":52,"dataId":14,"value":1}],"initialLevel":30,"maxLevel":90,"name":"死神","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"和死灵同类型的魔物。因HP变成0而发挥本领。经常处于不死状态。\n物理、魔法双方都拥有高度战斗力。但光是存在，我方全员的回复效果就会减半。"},{"id":32,"battlerName":"","characterIndex":2,"characterName":"ETNR_EX076","classId":26,"equips":[87,0,0,3,0],"faceIndex":5,"faceName":"Monster2","traits":[{"code":52,"dataId":16,"value":1}],"initialLevel":10,"maxLevel":32,"name":"食人魔","nickname":"","note":"<AddEquipSlot:4>\n<AddEquipSlot2:5, 2>","profile":"被称为战斗高手的魔物。所有武器都能装备。\n此外还取得了各种技能，但等级上限只到32级。"}]